{
  "profiles": {
    "Client": {
      "commandName": "Executable",
      "executablePath": "dotnet",
      "commandLineArgs": "\"$(VINTAGE_STORY)/Vintagestory.dll\" --tracelog --addModPath \"$(ProjectDir)/bin/$(Configuration)/Mods/\"",
      "workingDirectory": "$(VINTAGE_STORY)"
    },
    "Client - Ledger Only": {
      "commandName": "Executable",
      "executablePath": "dotnet",
      "commandLineArgs": "\"$(VINTAGE_STORY)/Vintagestory.dll\" --tracelog --addModPath \"$(ProjectDir)/bin/$(Configuration)/Mods/ledger/\"",
      "workingDirectory": "$(VINTAGE_STORY)"
    },
    "Test World (Flat)": {
      "commandName": "Executable",
      "executablePath": "dotnet",
      "commandLineArgs": "\"$(VINTAGE_STORY)/Vintagestory.dll\" --tracelog --addModPath \"$(ProjectDir)/bin/$(Configuration)/Mods/\" --playStyle \"creativebuilding\" --openWorld \"$(ProjectName) Test World (Flat)\"",
      "workingDirectory": "$(VINTAGE_STORY)"
    },
    "Server": {
      "commandName": "Executable",
      "executablePath": "dotnet",
      "commandLineArgs": "\"$(VINTAGE_STORY)/VintagestoryServer.dll\" --tracelog --addModPath \"$(ProjectDir)/bin/$(Configuration)/Mods/\" --playStyle \"creativebuilding\" --openWorld \"$(ProjectName) Test World (Flat)\"",
      "workingDirectory": "$(VINTAGE_STORY)"
    }
  }
}
